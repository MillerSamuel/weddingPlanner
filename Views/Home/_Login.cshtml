<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
@model LogUser
<body>
    <h1>Login</h1>
    <form action="/login" method="post">
        <div class="form-group">
            <label for="LogEmail">Email:</label>
            <input class="form-control" asp-for="LogEmail">
            <span asp-validation-for="LogEmail"></span>
        </div>
        <div class="form-group">
            <label for="LogPassword">Password:</label>
            <input class="form-control" asp-for="LogPassword">
            <span asp-validation-for="LogPassword"></span>
        </div>
        <input class="m-2" type="submit" value="Login">
    </form>
</body>
</html>